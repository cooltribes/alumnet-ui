<form class="formANet form" role="form">
	<h1 class="roundedNumbersList__textProfile">Aproval process
	<small class="hidden-xs">
		<%%= @approved_requests_count %>/3

		<small>
			Approved requests
		</small>
	</small>
	</h2>
	<p class="lead hidden-xs">Before you can enter the AlumNet platform you have to pass through an approval process where other AIESEC Alumni will certify that you are another AIESEC Alumni fellow.</p>
	<hr class="hidden-xs">
	<div class="row">
		<div class="col-md-2 col-xs-3 formANet__find">
			<%= image_tag("registration/searchIcon.png", alt: "question mark", class: "img-circle") %>
		</div>
		<div class="col-xs-9 visible-xs formANet__text margin_bottom">
			<h4>Find 3 People</h4>
			<small>Who can confirm that you are an AIESEC Alumni</small >
		</div>
		<div class="col-md-10">
			<h3 class="hidden-xs">Find 3 People</h3>
			<p class="hidden-xs">Who can confirm that you are an AIESEC Alumni</p >
			<div id="byService" class="importContactForm margin_top text-center hidden-xs">
			  <p>Find your friends in ALUMNET with</p>
			  <a class="deep-link delayed" onclick="return cloudsponge.launch('linkedin');">
			    <%= image_tag("social_contacts/linkedin.png", alt: "linkedIn") %>
			  </a>
			  <a class="deep-link delayed" onclick="return cloudsponge.launch('yahoo');">
			    <%= image_tag("social_contacts/yahoo.png", alt: "Yahoo") %>
			  </a>
			  <a class="deep-link delayed" onclick="return cloudsponge.launch('windowslive');">
			    <%= image_tag("social_contacts/outlook.com.png", alt: "Windows Live") %>
			  </a>
			  <a class="deep-link delayed" onclick="return cloudsponge.launch('gmail');">
			    <%= image_tag("social_contacts/gmail.png", alt: "Gmail") %>
			  </a>
			</div>
			<div class="form-group has-feedback">
				<label class="control-label">Search by name</label>
				<input type="text" class="form-control input-lg" placeholder="Write a name or email" name="search_term" id="search_term">
				<span class=" hidden-xs formANet__input__icon glyphicon glyphicon-user form-control-feedback">
			</div>
			<div class="row">
				<!-- <div class="col-md-6 col-xs-12">
					<div class="form-group">
						<label class="control-label">Search by Country</label>
						<div class="formANet__select">
			        <input id="js-residence-countries" type="hidden" name="residence_country_id" style="width: 100%">
						</div>
					</div>
				</div> -->

				<div class="col-md-6 col-md-offset-6 col-xs-12 text-right">
				  <div class="form-group padding_top_small">
							<button class="btn btn-primary btn-lg text-uppercase js-search" type="submit"> <span class="glyphicon glyphicon-search"></span> Search</button>
					</div>
			  </div>


			</div>
			<div class="text-right">
			<!-- <button class="btn btn-primary btn-lg text-uppercase js-search" type="submit"> <span class="glyphicon glyphicon-search"></span> Search</button> -->
			</div>
			<hr>
			<h3>Results</h3>
			<div class="users-list">

			</div>


			<!-- <button class="btn btn-default text-uppercase btn-primary-noBackground " type="submit"><span class=" icon-entypo-down-open "> SHOW MORE (3)</button> -->
		</div>
	</div>
	<hr>
	<div class="row">
		<div class="col-md-2 col-xs-12 formANet__find">
			<%= image_tag("registration/questionIcon.png", alt: "question mark", class: "img-circle") %>
			<!--<img src="assets/registration/questionIcon.png" alt="question mark"  width="90px" height="90px" class="img-circle">-->
		</div>
		<div class="col-md-10 col-xs-12 formANet__textHelp">
			<h3 >Didnt find anyone?</h3>
			<p class="lead" >Ask an administrator to approve you manually</p>
			<div class="js-buttonContainer formANet__buttonSend">
				<button class="btn btn-primary btn-lg text-uppercase js-askAdmin" type="button">SEND YOUR REQUEST TO ADMIN</button>
			</div>
		</div>
	</div>
</form>