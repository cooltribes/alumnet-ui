<td><%= @productType %></td>
<td><%= @product.name %></td>
<td><%= @startDate %></td>
<td class="js-end-date"><%= @endDate %></td>
<td>
	<%= @status %>
	<% if @status == "Active": %>
		<a class="js-deactivate" id="<%= @id %>">Deactivate</a>
	<% else: %>
		<a class="js-activate" id="<%= @id %>">Activate</a>
	<% end %>
</td>